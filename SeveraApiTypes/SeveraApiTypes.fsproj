<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Mutex.Visma.Severa.API.Types.ForUI</PackageId>
    <Version>0.1.2</Version>
    <Authors>Jarmo Muukka, Mutex Oy</Authors>
    <Company>Mutex Oy</Company>
    <Product>Visma Severa</Product>
    <Description>Contains Visma Severa REST API client types from v0.2 Swagger documentation for UI generated by SwaggerProvider 0.10.0.</Description>
    <Copyright>Copyright © Mutex Oy 2020</Copyright>
    <PackageTags>Visma Severa F# fsharp</PackageTags>
    <AssemblyName>Mutex.Visma.Severa.API.Types.ForUI</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md" />
    <Compile Include="SeveraApi.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SwaggerProvider" Version="0.10.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
